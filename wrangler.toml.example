# Example wrangler.toml - copy to wrangler.toml and fill in your values
# DO NOT commit wrangler.toml with real database IDs

name = "daily-grid-hq"
compatibility_date = "2025-07-18"
pages_build_output_dir = "./public"

# D1 database binding
# Get your database_id from: wrangler d1 list
[[d1_databases]]
binding = "DB"
database_name = "daily-grid-hq-db"
database_id = "YOUR_DATABASE_ID_HERE"
